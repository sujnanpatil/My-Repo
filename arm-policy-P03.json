{
	 "if": {
	    "anyOf": [
		 {
			"not": {
			  "anyOf": [
                    {
				    "field": "type",
				    "like": "microsoft.alertsmanagement/smartDetectorAlertRules/*"
				    },
					{
				    "field": "type",
				    "like": "Microsoft.OperationalInsights/workspaces/*"
				    },
                    {
				    "field": "type",
				    "like": "microsoft.insights/actiongroups/*"
				    },
                    {
				    "field": "type",
				    "like": "microsoft.insights/autoscalesettings/*"
				    },
                    {
				    "field": "type",
				    "like": "microsoft.insights/components/*"
				    },
                    {
				    "field": "type",
				    "like": "Microsoft.Web/serverFarms/*"
				    },
                    {
				    "field": "type",
				    "like": "Microsoft.Web/sites/*"
				    },
                    {
				    "field": "type",
				    "in": ["microsoft.alertsmanagement/smartDetectorAlertRules", "Microsoft.OperationalInsights/workspaces", "microsoft.insights/actiongroups", "microsoft.insights/autoscalesettings", "microsoft.insights/components", "Microsoft.Web/serverFarms", "Microsoft.Web/sites"]
				    }
                ]
			}
		 },
          {
		  "allof": [
			{
			  "field": "type",
			  "equals": "Microsoft.Web/serverfarms"
			},
			{
			  "not": {
				"field": "Microsoft.Web/serverfarms/sku.name",
				"in": ["F1"]
				}
			  }
			]
		  }

	  ]
	},
	"then": {
	  "effect": "deny"
	}
}
